{
  "name": "razzle-examples-with-typeorm-graphql",
  "version": "4.0.0-canary.37",
  "license": "MIT",
  "scripts": {
    "start": "razzle start",
    "build": "razzle build",
    "babel-build": "babel ts-src --out-dir src --extensions \".ts,.tsx\" --source-maps inline --keep-file-extension --config-file ./.babelrc.cli && yarn renametsx && yarn renamets && yarn movestatic",
    "renametsx": "renamer --find tsx --replace jsx \"src/**\"",
    "renamets": "renamer --find ts --replace js \"src/**\"",
    "movestatic": "find ts-src/ -type f -not -name \"*.tsx\" -not -name \"*.ts\" -exec bash -c 'echo \"${0##*/}\" \"${0##ts-src/}\" \"$0\"' {} \\; | xargs -l bash -c 'mkdir -p \"src/${1%$0}\" && cp \"$2\" \"src/$0\"'",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js --config ormconfig.json"
  },
  "dependencies": {
    "apollo-server-express": "^2.19.2",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.30"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0-0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@testing-library/dom": "^7.29.4",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.3",
    "@testing-library/user-event": "^10.1.0",
    "@types/express": "^4.17.7",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.2",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-router-dom": "^5.1.4",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "babel-preset-razzle": "4.0.0-canary.37",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.2.1",
    "razzle": "4.0.0-canary.37",
    "razzle-dev-utils": "4.0.0-canary.37",
    "razzle-plugin-typescript": "4.0.0-canary.37",
    "renamer": "^2.0.1",
    "sqlite3": "^5.0.1",
    "typescript": "^4.0.3",
    "webpack": "^5.16.0",
    "webpack-dev-server": "^3.11.0"
  }
}
